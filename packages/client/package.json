  "license": "MIT",
  "bugs": {
    "url": "https://github.com/galileo-dart/galileo_client/issues"
  },
  "homepage": "https://github.com/galileo-dart/galileo_client#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0"
  }
}
